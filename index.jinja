<html>
<head>
  <style>
    .thumb {
      float: left;
      margin-left: 10px;
      text-align: center;
      min-width: 150px;
      min-height: 150px;
    }
  </style>
</head>
<body>
<h1>Index</h1>
{% for image in images %}
<div class="thumb">
<a href="{{image.page_path}}"><img src="{{image.sm_thumb_path}}"></a>
<p>{{image.meta.title}}<br>
{{image.get_datetime_str()}}
</p>
</div>
{% endfor %}
</body>
</html>
